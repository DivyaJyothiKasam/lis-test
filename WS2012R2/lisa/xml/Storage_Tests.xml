<?xml version="1.0" encoding="utf-8"?>

<config>
    <global>
        <logfileRootDir>TestResults</logfileRootDir>
        <defaultSnapshot>ICABase</defaultSnapshot>
        <LisaInitScript>
            <file>.\setupScripts\CreateVMs.ps1</file>
        </LisaInitScript>
        <imageStoreDir>\\unc\path</imageStoreDir>
        <email>
            <recipients>
                <to>myself@mycompany.com</to>
            </recipients>
            <sender>myself@mycompany.com</sender>
            <subject>LIS Storage tests</subject>
            <smtpServer>mysmtphost.mycompany.com</smtpServer>
        </email>
    </global>

    <testSuites>
        <suite>
            <suiteName>Storage</suiteName>
            <suiteTests>
                <suiteTest>MainVM_Checkpoint</suiteTest>
                <!-- VHD tests -->
                <suiteTest>VHD_IDE_Dynamic</suiteTest>
                <suiteTest>VHD_IDE_Fixed</suiteTest>
                <suiteTest>VHD_AddDiff_Disk_IDE</suiteTest>
                <suiteTest>VHD_AddDiff_Disk_SCSI</suiteTest>
                <suiteTest>VHD_MultiDisk_SCSI</suiteTest>
                <suiteTest>VHD_HotADD_Multi_Dynamic_SCSI</suiteTest>
                <suiteTest>VHD_HotRemove_Multi_Dynamic_SCSI</suiteTest>
                <suiteTest>VHD_AddPassThrough_Disk_IDE</suiteTest>
                <suiteTest>VHD_HotADD_Multi_PassThrough_SCSI</suiteTest>
                <suiteTest>VHD_HotRemove_Multi_PassThrough_SCSI</suiteTest>
                <suiteTest>VHD_SCSI_Dynamic_Large_Disk</suiteTest>
                <!-- VHDx tests -->
                <!-- Windows Server 2012 and older do not support VHDx -->
                <suiteTest>VHDx_IDE_Dynamic</suiteTest>
                <suiteTest>VHDx_IDE_Fixed</suiteTest>
                <suiteTest>VHDx_AddDiff_Disk_IDE</suiteTest>
                <suiteTest>VHDx_AddDiff_Disk_SCSI</suiteTest>
                <suiteTest>VHDx_MultiDisk_SCSI</suiteTest>
                <suiteTest>VHDx_HotADD_Multi_Dynamic_SCSI</suiteTest>
                <suiteTest>VHDx_HotRemove_Multi_Dynamic_SCSI</suiteTest>
                <suiteTest>VHDx_Hot_Unplug_Plug</suiteTest>
                <suiteTest>VHDx_Hot_Plug_Unplug_Multi_SCSI</suiteTest>
                <suiteTest>VHDx_4k_IDE</suiteTest>
                <suiteTest>VHDx_4k_Multi_Dynamic_SCSI</suiteTest>
                <suiteTest>VHDx_4k_HotADD_Multi_Dynamic_SCSI</suiteTest>
                <suiteTest>VHDx_4k_HotRemove_Multi_Dynamic_SCSI</suiteTest>
                <suiteTest>VHDx_SCSI_Dynamic_Large_16TB</suiteTest>
                <!-- 64TB partition requires a 64-bit Guest OS -->
                <suiteTest>VHDx_SCSI_Dynamic_Large_64TB</suiteTest>
                <suiteTest>VHDx_SCSI_Dynamic_local_copy_file</suiteTest>
                <suiteTest>VHDx_SCSI_Dynamic_wget_file</suiteTest>
                <suiteTest>VHDx_Verify_SectorSize</suiteTest>
                <suiteTest>Take_Revert_Snapshot</suiteTest>
                <suiteTest>Export_Import_VM</suiteTest>
                <!-- Below test cases require either iSCSI or NFS setup
                <suiteTest>Make_passthrough_offline</suiteTest>
                <suiteTest>VHDx_Mount_NFS</suiteTest>
                <suiteTest>VHDx_SCSI_Dynamic_NFS_File</suiteTest> -->
            </suiteTests>
        </suite>
    </testSuites>
